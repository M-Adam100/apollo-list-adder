{
    "name": "Apollo List Adder",
    "version": "1.0",
    "description": "",
    "manifest_version": 3,
    "incognito": "split",
    "icons": {
        "128": "icons/128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "icons/128.png",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "scripting",
        "storage"
    ],
    "host_permissions": [
      "*://*.app.apollo.io/*"
     ]
    
}